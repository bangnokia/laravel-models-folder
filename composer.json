{
    "name": "bangnokia/laravel-models-folder",
    "description": "Create laravel models into Models folder by default",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "models folders"],
    "authors": [
        {
            "name": "Nguyen Viet",
            "email": "bangnokia@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/console": "^5"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "BangNokia\\LaravelModelsFolder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BangNokia\\LaravelModelsFolder\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BangNokia\\LaravelModelsFolder\\ModelsFolderServiceProvider"
            ]
        }
    }
}
